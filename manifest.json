{
  "name": "Force Redraw IMG on HTML5 Canvas (beta)",
  "manifest_version": 2,
  "version": "0.17.7.3",
  "description": "Redraw IMG elements on HTML5 Canvas, forcing GPU to handle the work.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Force Redraw IMG on Canvas",
      "default_icon": "icon.png"
  }
}
